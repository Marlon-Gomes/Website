  3.771853912176478*^9}},ExpressionUUID->"125a5b4e-b4e8-4e17-8221-\
71153138fc62"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"TraditionalForm", "[", 
  RowBox[{
   RowBox[{"HoldForm", "[", 
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Log", "[", "y", "]"}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"Sqrt", "[", "y", "]"}], ")"}]}], ",", "y"}], "]"}], "]"}], 
   "\[Equal]", " ", 
   RowBox[{
    RowBox[{"2", 
     RowBox[{"Log", "[", "y", "]"}], "*", 
     RowBox[{"Sqrt", "[", "y", "]"}]}], "-", 
    RowBox[{"HoldForm", "[", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"1", "/", "y"}], "*", "2", 
        RowBox[{"Sqrt", "[", "y", "]"}]}], ",", "y"}], "]"}], "]"}]}], 
   "\[Equal]", " ", 
   RowBox[{
    RowBox[{"2", 
     RowBox[{"Log", "[", "y", "]"}], "*", 
     RowBox[{"Sqrt", "[", "y", "]"}]}], "-", " ", 
    RowBox[{"HoldForm", "[", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{"2", "/", 
        RowBox[{"Sqrt", "[", "y", "]"}]}], ",", "y"}], "]"}], "]"}]}], 
   "\[Equal]", " ", 
   RowBox[{
    RowBox[{"2", 
     RowBox[{"Log", "[", "y", "]"}], "*", 
     RowBox[{"Sqrt", "[", "y", "]"}]}], " ", "-", 
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{"2", "/", 
       RowBox[{"Sqrt", "[", "y", "]"}]}], ",", "y"}], "]"}], "+", "C"}]}], 
  " ", "]"}]], "Input",
 CellChangeTimes->{{3.771853868588619*^9, 3.771853876317482*^9}, {
  3.771853919034886*^9, 3.7718541163152246`*^9}},
 CellLabel->
  "In[321]:=",ExpressionUUID->"70331e1a-d59e-48db-94f9-2a1bdea35d4d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   TagBox[
    RowBox[{"\[Integral]", 
     RowBox[{
      FractionBox[
       RowBox[{"log", "(", "y", ")"}], 
       SqrtBox["y"]], 
      RowBox[{"\[DifferentialD]", "y"}]}]}],
    HoldForm], "\[LongEqual]", 
   RowBox[{
    RowBox[{"2", " ", 
     SqrtBox["y"], " ", 
     RowBox[{"log", "(", "y", ")"}]}], "-", 
    TagBox[
     RowBox[{"\[Integral]", 
      RowBox[{
       FractionBox[
        RowBox[{"2", " ", 
         SqrtBox["y"]}], "y"], 
       RowBox[{"\[DifferentialD]", "y"}]}]}],
     HoldForm]}], "\[LongEqual]", 
   RowBox[{
    RowBox[{"2", " ", 
     SqrtBox["y"], " ", 
     RowBox[{"log", "(", "y", ")"}]}], "-", 
    TagBox[
     RowBox[{"\[Integral]", 
      RowBox[{
       FractionBox["2", 
        SqrtBox["y"]], 
       RowBox[{"\[DifferentialD]", "y"}]}]}],
     HoldForm]}], "\[LongEqual]", 
   RowBox[{"C", "-", 
    RowBox[{"4", " ", 
     SqrtBox["y"]}], "+", 
    RowBox[{"2", " ", 
     SqrtBox["y"], " ", 
     RowBox[{"log", "(", "y", ")"}]}]}]}], TraditionalForm]], "Output",
 CellChangeTimes->{
  3.771854027613544*^9, {3.771854088268606*^9, 3.7718541177001743`*^9}},
 TextAlignment->Center,
