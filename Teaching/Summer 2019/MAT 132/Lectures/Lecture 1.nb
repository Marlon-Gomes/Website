Cell[BoxData[
 RowBox[{"TraditionalForm", "[", 
  RowBox[{"HoldForm", "[", 
   RowBox[{
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{"x", "^", "2"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}], "\[Equal]", 
    RowBox[{"Limit", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"i", "^", "2"}], "/", 
         RowBox[{"n", "^", "3"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}], ",", 
      RowBox[{"n", "\[Rule]", " ", "\[Infinity]"}]}], "]"}], "\[Equal]", " ", 
    
    RowBox[{"Limit", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "*", 
           RowBox[{"(", 
            RowBox[{"n", "+", "1"}], ")"}], "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "n"}], "+", "1"}], ")"}]}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{"6", 
           RowBox[{"n", "^", "3"}]}], ")"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}], ",", 
      RowBox[{"n", "\[Rule]", " ", "\[Infinity]"}]}], "]"}], "\[Equal]", 
    RowBox[{"1", "/", "3"}]}], " ", "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.7716046516976213`*^9, 3.7716048202940207`*^9}, {
  3.7716050123107567`*^9, 3.7716050334616013`*^9}, {3.771605096009305*^9, 
